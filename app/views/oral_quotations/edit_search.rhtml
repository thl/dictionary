<% if params['relatedtype'] == "definition" %>
  <div id="oral_quotation_definitions">
  <% form_remote_tag(:update => "oral_quotation_definitions", :url => {:controller => "oral_quotations", :action => "edit_search_action", :id => params['id'], :params => {:level => params['level'], :internal => params['internal'], :pk => params['pk']}}, :html =>{:name => "related"}) do %>
    <h3>Find Definition:</h3>
  <%= link_to_function(image_tag("search_up.gif", :size => "87x19", :border => 0, :alt => "search", :name => "oral_quotationsearch1"+params['level'].to_s), "if(document.related.onsubmit()) { document.related.submit();} ", {:onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('oral_quotationsearch1"+params['level'].to_s+"','','/images/search_down.gif',1);"}) %>
