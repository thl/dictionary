<% if params['relatedtype'] == "translation" %>
<div id="model_sentence_translations">

<% form_remote_tag(:update => 'definition_space', :complete => "new Effect.Fade('edit_box');", :url => {:controller => "model_sentences", :action => 'edit_add_to_list_action', :id => params['id'],:params =>{ :internal => params['internal'],:test => 'true'}}, :html =>{:name => "related"}) do %>

  <h3>Check Translation to Add:</h3>
