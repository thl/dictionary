<% if params['relatedtype'] == "definition" %>
<div id="model_sentence_definitions">
<% form_remote_tag(:update => "model_sentence_definitions", :url => {:controller => "model_sentences", :action => 'edit_add_to_list_action', :id => params['id']}, :html =>{:name => "related"}) do %>
  <h3>Check Definition to Add:</h3>
<br><%= link_to_function(image_tag("update_up.gif", :size => "87x19", :border => 0, :alt => "update", :name => "model_sentencelistupdate4"+params['level'].to_s), "if(document.related.onsubmit()) { document.related.submit();}", {:onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('model_sentencelistupdate4"+params['level'].to_s+"','','/images/update_down.gif',1);"}) %>
  <%= link_to_remote image_tag("cancel_up.gif", :size => "87x19", :border => 0, :alt => "cancel", :name => "model_sentencequerycancel0"+params['level'].to_s), {:update => 'model_sentence_definitions',  :url => {:controller => 'model_sentences', :action => 'cancel', :params => {:pk => params[:id], 'internal' => 'model_sentences', 'relatedtype'=> params['relatedtype'], 'level' => params['level']}}}, :onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('model_sentencequerycancel0"+params['level'].to_s+"','','/images/cancel_down.gif',1);"%><br>
   <% if @model_sentence.definition != nil %>
     <p><%= build_radiolist_group(@definitions,@model_sentence.definition.displayInfo) %></p>
   <% else %>
     <p><%= build_radiolist_group(@definitions) %></p>
   <% end %>
   <input type=hidden name=relatedtype value=definition />
<% end %>
<input type="hidden" name="model_sentence[id]" value="<%= @model_sentence.id %>"/> 
<br><%= link_to_function(image_tag("update_up.gif", :size => "87x19", :border => 0, :alt => "update", :name => "model_sentencelistupdate3"+params['level'].to_s), "if(document.related.onsubmit()) { document.related.submit();}", {:onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('model_sentencelistupdate3"+params['level'].to_s+"','','/images/update_down.gif',1);"}) %>
  <%= link_to_remote image_tag("cancel_up.gif", :size => "87x19", :border => 0, :alt => "cancel", :name => "model_sentencequerycancel"+params['level'].to_s), {:update => 'model_sentence_definitions',  :url => {:controller => 'model_sentences', :action => 'cancel', :params => {:pk => params[:id], 'internal' => 'model_sentences', 'relatedtype'=> params['relatedtype'], 'level' => params['level']}}}, :onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('model_sentencequerycancel"+params['level'].to_s+"','','/images/cancel_down.gif',1);"%><br>
</div>
<% end %>
