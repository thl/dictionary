<% if !@translations.nil? %>
<% if @translations.size > 10 %>
<div id="scrollwindow" style="width:600;height:200;background-color:e6e6e6;overflow:auto">
<% end %>
   <% if @model_sentence.translations != nil %>
     <%= build_checklist_group(@translations,@model_sentence.translations) %>
   <% else %>
     <p><%= build_checklist_group(@model_sentence.translations) %></p>
   <% end %>
<% if @translations.size > 10 %>
</div>
