<%= wide_tagged_tinymce_script %>
<% form_for :etymology, :url => { :action => "update_popupetymology" } do |f| %>
<p>
<%= f.hidden_field :id %>
<%= f.text_area :etymology, :class => 'mceEditor' %>
<%= submit_to_remote 'update_button','update etymology', :url => { :action => "update_popupetymology" }, :update => "#{e.id}_etypopupdiv", :before => "tinyMCE.triggerSave();" %>
<%#= link_to_remote 'Cancel', :url => {:action => "etymology_show", :params => {'id' => params['id'] }}, :update => "#{e.id}_etydiv" %>
<%= link_to_remote 'Cancel', {:url => etymology_popupshow_path(params['id']), :method => 'get', :update => "#{e.id}_etypopupdiv" } %>
</p>
<% end %>
