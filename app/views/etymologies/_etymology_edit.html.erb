<%= tagged_tinymce_script %>
<% form_for :etymology, :url => { :action => "update_etymology" } do |f| %>
<p>
<%= f.hidden_field :id %>
<%= f.text_area :etymology, :class => 'mceEditor' %>
<%= submit_to_remote 'update_button','update etymology', :url => { :action => "update_etymology" }, :update => "#{e.id}_etydiv", :before => "tinyMCE.triggerSave();" %>
<%= link_to_remote 'Cancel', :url => {:action => "etymology_show", :params => {'id' => params['id'] }}, :update => "#{e.id}_etydiv" %>
</p>
<% end %>
