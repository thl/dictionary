<% if params['relatedtype'] == "theme_level_two" %>
<div id="theme_level_three_theme_level_twos">
<%= form_remote_tag(:update => "theme_level_three_theme_level_twos", :url => {:controller => "theme_level_threes", :action => 'edit_add_to_list_action', :id => params['id']}, :name => "listrelated") %>
  <h3>Check Theme level two to Add:</h3>
<br><%= link_to_function(image_tag("update_up.gif", :size => "87x19", :border => 0, :alt => "update", :name => "theme_level_threelistupdate4"+params['level'].to_s), "if(document.forms["+params['level'].to_s+"].onsubmit()) { document.forms["+params['level'].to_s+"].submit();}", {:onMouseOut => "MM_swapImgRestore();", :onMouseOver => "MM_swapImage('theme_level_threelistupdate4"+params['level'].to_s+"','','/images/update_down.gif',1);"}) %>
