<h1>Listing category_translation_associations</h1>

<table>
  <tr>
    <th>Translation</th>
    <th>Category branch</th>
    <th>Category</th>
  </tr>

<% @category_translation_associations.each do |category_translation_association| %>
  <tr>
    <td><%=h category_translation_association.translation_id %></td>
    <td><%=h category_translation_association.category_branch_id %></td>
    <td><%=h category_translation_association.category_id %></td>
    <td><%= link_to 'Show', category_translation_association %></td>
    <td><%= link_to 'Edit', edit_category_translation_association_path(category_translation_association) %></td>
    <td><%= link_to 'Destroy', category_translation_association, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

